<body>


    @if (User.IsInRole("User"))
    {
        <!-- ======= Header ======= -->
        <header id="header" class="fixed-top">
            <div class="container d-flex align-items-center justify-content-lg-between">

                <h1 class="logo me-auto me-lg-0"><a asp-controller="Home" asp-action="Index"><span>Jeep</span></a></h1>

                <nav id="navbar" class="navbar order-last order-lg-0">
                    <ul>
                        <li><a class="nav-link scrollto active" asp-action="Index" asp-controller="Home">Home</a></li>
                        <li><a class="nav-link scrollto" href="#services">Services</a></li>
                        <li><a class="nav-link scrollto" href="#about">About</a></li>
                        <li><a class="nav-link scrollto" asp-action="IndexForUser" asp-controller="Cars">Booking</a></li>
                        <li><a class="nav-link scrollto" asp-action="IndexForUser" asp-controller="Books">Requests</a></li>

                    </ul>
                    <i class="bi bi-list mobile-nav-toggle"></i>
                </nav><!-- .navbar -->
                @if (User.Identity.IsAuthenticated)
                {
                    <div class="dropdown">
                        <a class="get-started-btn scrollto dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            @User.Identity.Name
                        </a>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                            <a asp-action="Logout" asp-controller="Home" class="dropdown-item">Logout</a>
                        </div>
                    </div>
                }
                else
                {
                    <a asp-action="Login" asp-controller="Home" class="get-started-btn scrollto">Log in</a>
                }
            </div>
        </header>

        <!-- End Header -->
    }


    @if (User.IsInRole("Admin"))
    {
        <!-- ======= Header ======= -->
        <header id="header" class="fixed-top">
            <div class="container d-flex align-items-center justify-content-lg-between">
                <h1 class="logo me-auto me-lg-0"><a asp-controller="Home" asp-action="Index"><span>Jeep</span></a></h1>
                <nav id="navbar" class="navbar order-last order-lg-0">
                    <ul>
                        <li><a class="nav-link scrollto active" asp-controller="Home" asp-action="IndexAdmin">Home</a></li>
                        <li><a class="nav-link scrollto" asp-controller="Cars" asp-action="Index">Cars</a></li>
                        <li class="dropdown">
                            <a href="#"><span>Users</span> <i class="bi bi-chevron-down"></i></a>
                            <ul>
                                <li><a class="nav-link scrollto" asp-controller="Users" asp-action="Index">User</a></li>
                                <li><a class="nav-link scrollto" asp-controller="Users" asp-action="Admin">Admin</a></li>
                            </ul>
                        </li>
                        <li><a class="nav-link scrollto" asp-action="Index" asp-controller="Books">Requests</a></li>

                    </ul>
                    <i class="bi bi-list mobile-nav-toggle"></i>
                </nav><!-- .navbar -->
                @if (User.Identity.IsAuthenticated)
                {
                    <div class="dropdown">
                        <a class="get-started-btn scrollto dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            @User.Identity.Name
                        </a>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                            <a asp-action="Logout" asp-controller="Home" class="dropdown-item">Logout</a>
                        </div>
                    </div>
                }
                else
                {
                    <a asp-action="Login" asp-controller="Home" class="get-started-btn scrollto">Log in</a>
                }
            </div>
        </header>

        <!-- End Header -->
    }

    @if (!User.Identity.IsAuthenticated)
    {
        <header id="header" class="fixed-top">
            <div class="container d-flex align-items-center justify-content-lg-between">

                <h1 class="logo me-auto me-lg-0"><a asp-controller="Home" asp-action="Index"><span>Jeep</span></a></h1>


                <!-- Uncomment below if you prefer to use an image logo -->
                <!-- <a href="index.html" class="logo me-auto me-lg-0"><img src="~/assets/img/logo.png" alt="" class="img-fluid"></a>-->

                <nav id="navbar" class="navbar order-last order-lg-0">
                    <ul>
                        <li><a class="nav-link scrollto active" asp-action="Index" asp-controller="Home">Home</a></li>
                        <li><a class="nav-link scrollto" href="#services">Services</a></li>
                        <li><a class="nav-link scrollto" href="#about">About</a></li>
                        <li><a class="nav-link scrollto" asp-action="IndexForUser" asp-controller="Cars">Booking</a></li>


                    </ul>
                    <i class="bi bi-list mobile-nav-toggle"></i>
                </nav><!-- .navbar -->
                @if (User.Identity.IsAuthenticated)
                {
                    <div class="dropdown">
                        <a class="get-started-btn scrollto dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            @User.Identity.Name
                        </a>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                            <a asp-action="Logout" asp-controller="Home" class="dropdown-item">Logout</a>
                        </div>
                    </div>
                }
                else
                {
                    <a asp-action="Login" asp-controller="Home" class="get-started-btn scrollto">Log in</a>
                }
            </div>
        </header>

        <!-- End Header -->
    }



    <main role="main" class="pb-3">
        @RenderBody()
    </main>



    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="copyright">
            &copy; Copyright <strong><span>Jeep</span></strong>. All Rights Reserved
        </div>
       @* <div class="credits">

            Dev by <a href="#"></a>
        </div>*@
    </footer><!-- End Footer -->

    <div id="preloader"></div>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/assets/vendor/aos/aos.js"></script>
    <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/assets/js/main.js"></script>







</body>

